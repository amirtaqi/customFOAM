    "(phiEs|phiE)"
    {
    	solver GAMG;
    	tolerance 1e-18;
    	relTol 0.0;
    	smoother GaussSeidel;
    	nPreSweeps 0;
    	nPostSweeps 2;
    	nFinestSweeps 2;
    	cacheAgglomeration false;
    	nCellsInCoarsestLevel 5;
    	agglomerator faceAreaPair;
    	mergeLevels 1;
    }

    "(phiEs|phiE)Final"
    {
   	    $phiE;
    	tolerance 1e-18;
    	relTol 0;
    }

    p 
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0;
    }
    pFinal
    {
    	$p;
    	tolerance 1e-12;
    	relTol 0;
    }

    phi
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0;
    }
    "U"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-08;
        relTol          0.01;
    }

    "UFinal"
    {
        $U;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0;
    }
    "(voidfraction|Ksl|UsNext|voidfractionNext|sSmoothField|vSmoothField|fSmooth)"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-08;
        relTol          0;
    }

    Ci
