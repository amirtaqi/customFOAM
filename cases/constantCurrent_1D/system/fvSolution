solvers
{
    rho
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-08;
        relTol          0;
    }

    rhoFinal
    {
	    $rho;
	    tolerance 1e-12;
	    relTol 0;
    }
    h
