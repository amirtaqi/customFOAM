FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     electrochemicalFoam;
//application     cfdemSolverPiso;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1; 

deltaT          1e-3;

writeControl    timeStep; //adjustableRunTime;//

writeInterval   100;//10;//0.05;//

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable off;//yes;

adjustTimeStep  no;

maxCo           1;

maxDeltaT       1;


libs 
( 
    "libextendedBoundaryConditions.so"
    "libextendedChemistryModel.so"
    "libelectrochemistryModels.so"
    //"libvolWeighted.so"
