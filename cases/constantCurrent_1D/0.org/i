FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      i;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 0 0 0 1 0];

internalField   uniform (0 0 0);

boundaryField
{
    topAndBottom
    {
        type            empty;
    }
    frontAndBack
    {
        type            empty;
    }
    currentCollector
    {
        type            fixedValue;
        value           uniform (1000 0 0);
    }
    separator
    {
        type            zeroGradient;
    }
}


